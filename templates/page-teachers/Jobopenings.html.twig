{% extends 'main_page/index.html.twig' %}
{% block title %}Вакансии{% endblock %}
{% block main %}
    <h1 class="label-contactPod">Вакансии</h1>
    <div class="label-contactPod" >
        <div>
{% for pageCont in pageData %}
    {% if pageCont is not empty %}
            <div class="tab-page-student">
                
            <input id="tab-one{{ pageCont.id|raw }}" type="checkbox" name="tabs">
                <label  for="tab-one{{ pageCont.id|raw }}">{{ pageCont.title|raw }}</label>
                    <div class="tab-content">
                    <div class="tab-content-first-page-student">
                        <p>{{ pageCont.text|raw }}</p>
                    </div>
                    </div>
            </div>
            </div>
    {% endif %}
{% endfor %}

        </div>
    </div>
{% endblock %}